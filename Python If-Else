import math
import os
import random
import re
import sys



if __name__ == '__main__':
    n = int(input().strip())
W = "Weird"
nW = "Not Weird"
if (1 <= n <= 100):
    if (n % 2 != 0):
        print(W)
        
    else:
        if (2 <= n <= 5):
            print(nW)
        if (6 <= n <= 20):
            print(W)
        if (20 < n):
            print(nW)
